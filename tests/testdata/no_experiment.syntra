dataset dummy : ImageDataset {
    source   = "data/dummy.csv"
    shape    = (28, 28)
    channels = 1
    batch    = 32
    shuffle  = true
    seed     = 123
}

model tiny_cnn : TorchModel {
    arch       = "cnn"
    framework  = "pytorch"
    lr         = 1e-3
    epochs     = 1
    optimizer  = "adam"
}
